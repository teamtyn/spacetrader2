<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="720.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="spacetrader2.MenuController" style="-fx-background-color: black;">
    <children>
      <VBox prefHeight="720.0" prefWidth="960.0">
         <children>
            <TextField alignment="CENTER" editable="false" prefHeight="100.0" prefWidth="960.0" text="Space Trader">
               <font>
                  <Font size="36.0" />
               </font>
            </TextField>
              <Button fx:id="startButton" alignment="CENTER" contentDisplay="CENTER" onAction="#startButtonAction" prefHeight="40.0" prefWidth="160.0" text="Start Game" textAlignment="CENTER">
               <VBox.margin>
                  <Insets left="400.0" top="150.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="exitButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#exitButtonAction" prefHeight="40.0" prefWidth="160.0" text="Exit Game" textAlignment="CENTER">
               <VBox.margin>
                  <Insets left="400.0" top="50.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
    </children>
</AnchorPane>
